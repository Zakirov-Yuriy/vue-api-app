{
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "http://109.73.206.144:6969/api/$1"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Access-Control-Allow-Origin",
          "value": "*"
        },
        {
          "key": "Access-Control-Allow-Methods",
          "value": "GET,OPTIONS"
        }
      ]
    }
  ],
  "build": {
    "env": {
      "VITE_API_BASE_URL": "/api",
      "VITE_API_KEY": "E6kUTYrYwZq2tN4QEtyzsbEBk3ie"
    }
  }
}